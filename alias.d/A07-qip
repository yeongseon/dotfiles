alias qip-cli="${QIPHOME}/cli/qip-cli -c";

alias allServers="echo qip-1dnses101.mandtbank.com qip-1dnses201.mandtbank.com \
    $(
        (
            (${QIPHOME}/cli/qip-getdnssvrlst -o "M&T Internal" -u "${LOGNAME}" -p "$(javaPasswordRepository --decrypt --entry QIPUSER --username ${LOGNAME} 2>/dev/null)" | /usr/bin/env grep qip | /usr/bin/env cut -d "," -f 1) && \
            (${QIPHOME}/cli/qip-getdnssvrlst -o "M&T External" -u "${LOGNAME}" -p "$(javaPasswordRepository --decrypt --entry QIPUSER --username ${LOGNAME} 2>/dev/null)" | /usr/bin/env grep qip | /usr/bin/env cut -d "," -f 1) && \
            (${QIPHOME}/cli/qip-getdhcpsvrlst -o "M&T Internal" -u "${LOGNAME}" -p "$(javaPasswordRepository --decrypt --entry QIPUSER --username ${LOGNAME} 2>/dev/null)" -all | /usr/bin/env grep qip | /usr/bin/env cut -d "," -f 1)
        ) | \
        /usr/bin/env sort | /usr/bin/env uniq
    )";

alias fixprinters="${QIPHOME}/cli/qip-dnsupdate -o \"M&T Internal\" -u \"${LOGNAME}\" -p \"$(javaPasswordRepository --decrypt --entry QIPUSER --username ${LOGNAME} 2>/dev/null)\" -n \"$(/usr/bin/env dig +short -t soa prod.mtb.com | /usr/bin/env awk '{print $1}')\"";

[ ! -z "${ENABLE_VERBOSE}" -a "${ENABLE_VERBOSE}" = "${_TRUE}" ] && set -x || set +x;
[ ! -z "${ENABLE_TRACE}" -a "${ENABLE_TRACE}" = "${_TRUE}" ] && set -v || set +v;

