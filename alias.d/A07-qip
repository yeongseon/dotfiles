alias qip-getloclst='/opt/qip/cli/qip-getloclst -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)" -t ~/workspace/locations.txt';
alias qip-getfreesubnetlst='/opt/qip/cli/qip-getfreesubnetlst -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)" -t new';
alias qip-getrtrlst='/opt/qip/cli/qip-getrtrlst -o "M&T Internal" -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)"';
alias qip-admin='/opt/qip/cli/qip-admin -o "M&T Internal" -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)"';
alias qip-cli='/opt/qip/cli/qip-cli -c';

## yeah why not
alias allServers='echo qip-1dnses101.mandtbank.com qip-1dnses201.mandtbank.com $( (( qip-getdnssvrlst -o "M&T Internal" | grep qip | cut -d "," -f 1) && (qip-getdnssvrlst -o "M&T External" | grep qip | cut -d "," -f 1) && (qip-getdhcpsvrlst -all | cut -d "," -f 1 | grep qip)) | sort | uniq)';

## ffs
alias fixprinters="/opt/qip/cli/qip-dnsupdate -o \"M&T Internal\" -u \"${LOGNAME}\" -p \"$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)\" -n $(dig +short -t soa prod.mtb.com | awk '{print $1}')";

[ ! -z "${ENABLE_VERBOSE}" -a "${ENABLE_VERBOSE}" = "${_TRUE}" ] && set -x || set +x;
[ ! -z "${ENABLE_TRACE}" -a "${ENABLE_TRACE}" = "${_TRUE}" ] && set -v || set +v;

