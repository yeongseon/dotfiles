[ ! -z "${ENABLE_VERBOSE}" -a "${ENABLE_VERBOSE}" = "${_TRUE}" ] && set -x || set +x;
[ ! -z "${ENABLE_TRACE}" -a "${ENABLE_TRACE}" = "${_TRUE}" ] && set -v || set +v;

[ ! -d /opt/qip ] && return;

alias qip-zoneserveroptions='/opt/qip/cli/qip-zoneserveroptions -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)"';
alias qip-setzoneprof='/opt/qip/cli/qip-setzoneprof -u "${LOGNAME}" -p "$(/usr/bin/env openssl enc -in ${HOME}/.etc/qip/password -d -aes-256-cbc -a -salt -pass file:${HOME}/.etc/salt 2>/dev/null)"';
