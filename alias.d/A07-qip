[ ! -z "${ENABLE_VERBOSE}" -a "${ENABLE_VERBOSE}" = "${_TRUE}" ] && set -x || set +x;
[ ! -z "${ENABLE_TRACE}" -a "${ENABLE_TRACE}" = "${_TRUE}" ] && set -v || set +v;

[ ! -d ${QIPHOME} ] && return;

alias qip-zoneserveroptions="${QIPHOME}/cli/qip-zoneserveroptions -u \"${LOGNAME}\" -p \"$(javaPasswordRepository --decrypt --entry QIPUSER --username ${LOGNAME} 2>/dev/null)\"";
